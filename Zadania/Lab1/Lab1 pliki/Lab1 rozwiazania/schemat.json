{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Generated schema for Root",
    "type": "object",
    "properties": {
        "dataPoczatkowa": {
            "type": "string",
            "format": "date"
        },
        "dataKoncowa": {
            "type": "string",
            "format": "date"
        },
        "pociagi": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "numer": {
                        "type": "integer"
                    },
                    "przewoznik": {
                        "type": "string",
                        "maxLength": 15
                    },
                    "rodzaj": {
                        "type": "string",
                        "enum": [
                            "osobowy",
                            "pospieszny",
                            "ekspres"
                        ]
                    },
                    "nazwa": {
                        "type": [
                            "string",
                            "null"
                        ]
                    },
                    "regionalny": {
                        "type": [
                            "boolean",
                            "null"
                        ]
                    },
                    "stacje": {
                        "type": "array",
                        "minItems": 2,
                        "items": {
                            "type": "object",
                            "properties": {
                                "nazwa": {
                                    "type": "string"
                                },
                                "przyjazd": {
                                    "type": "string"
                                },
                                "odjazd": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "nazwa",
                                "przyjazd",
                                "odjazd"
                            ]
                        }
                    },
                    "wagony": {
                        "type": "array",
                        "minItems": 1,
                        "maxItems": 5,
                        "items": {
                            "type": "string",
                            "enum": [
                                "klasa 1",
                                "klasa 2",
                                "sypialny",
                                "le≈ºakowy",
                                "restauracyjny"
                            ]
                        }
                    }
                },
                "required": [
                    "numer",
                    "przewoznik",
                    "rodzaj",
                    "regionalny",
                    "stacje",
                    "wagony"
                ]
            }
        }
    },
    "required": [
        "dataPoczatkowa",
        "dataKoncowa",
        "pociagi"
    ]
}